{
  "name": "dbgate",
  "version": "6.6.1-premium-beta.15",
  "private": true,
  "author": "Jan Prochazka <jenasoft.database@gmail.com>",
  "description": "Opensource database administration tool",
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-updater": "^6.3.4",
    "jsonwebtoken": "^9.0.2",
    "lodash.clonedeepwith": "^4.5.0",
    "patch-package": "^6.4.7",
    "@clickhouse/client": "^1.5.0",
    "wkx": "^0.5.0",
    "pg-copy-streams": "^6.0.6",
    "pg": "^8.11.5",
    "bson": "^6.8.0",
    "mongodb": "^6.3.0",
    "@mongosh/browser-runtime-electron": "^3.16.4",
    "@mongosh/service-provider-node-driver": "^3.10.2",
    "tedious": "^18.6.1",
    "mysql2": "^3.11.3",
    "ioredis": "^5.4.1",
    "node-redis-dump2": "^0.5.0",
    "@aws-sdk/rds-signer": "^3.665.0",
    "activedirectory2": "^2.1.0",
    "axios": "^0.21.1",
    "ssh2": "^1.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgate/dbgate.git"
  },
  "build": {
    "artifactName": "dbgate-${version}-${os}_${arch}.${ext}",
    "appId": "org.dbgate",
    "productName": "DbGate",
    "asarUnpack": "**/*.node",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.ico",
      "publish": [
        {
          "provider": "github",
          "owner": "dbgate",
          "repo": "dbgate"
        }
      ]
    },
    "files": [
      "packages",
      "src",
      "icon.png",
      "!node_modules/cpu-features/build/**"
    ]
  },
  "homepage": "./",
  "scripts": {
    "start": "cross-env ELECTRON_START_URL=http://localhost:5001 DEVMODE=1 electron .",
    "start:local": "cross-env electron .",
    "dist": "electron-builder",
    "build": "cd ../packages/api && yarn build && cd ../web && yarn build && cd ../../app && yarn dist",
    "build:local": "cd ../packages/api && yarn build && cd ../web && yarn build && cd ../../app && yarn predist",
    "postinstall": "yarn rebuild && patch-package",
    "rebuild": "electron-builder install-app-deps",
    "predist": "copyfiles ../packages/api/dist/* packages && copyfiles \"../packages/web/public/*\" packages && copyfiles \"../packages/web/public/**/*\" packages && copyfiles --up 3 \"../plugins/dist/**/*\" packages/plugins"
  },
  "main": "src/electron.js",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "cross-env": "^6.0.3",
    "electron": "37.2.6",
    "electron-builder": "26.0.12"
  },
  "optionalDependencies": {
    "@duckdb/node-api": "^1.2.1-alpha.16",
    "mongodb-client-encryption": "^6.1.1",
    "msnodesqlv8": "^5.1.1",
    "oracledb": "^6.6.0",
    "libsql": "0.5.0-pre.6",
    "better-sqlite3": "11.8.1"
  }
}